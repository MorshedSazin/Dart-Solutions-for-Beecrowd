import "dart:io";
void main(){
  int testCase = int.parse(stdin.readLineSync());// test case
  int j = 0;
  
  while(testCase>j){
    int readLine = int.parse(stdin.readLineSync());
    int i = 1;
    int sum = 0;
    while(i<readLine){
      if(readLine % i == 0){
    sum+=i;
    }i++; 
    
  }
  if(readLine==sum) print("${readLine} eh perfeito");
  else print("${readLine} nao eh perfeito");
  j++;
}
}
