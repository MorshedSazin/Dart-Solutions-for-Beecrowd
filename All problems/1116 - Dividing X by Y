import 'dart:io';

void main() {
  int n = int.parse(stdin.readLineSync());
  for (int i = 0; i < n; i++) {
    List<int> Dividing =
        stdin.readLineSync().split(" ").map(int.parse).toList();
    Dividing[0];
    Dividing[1];

    if (Dividing[1] == 0)
      print("divisao impossivel");
    else {
      double DividingXbyY = Dividing[0] / Dividing[1];
      print(DividingXbyY);
    }
  }
}
