import 'dart:io';
void main(){
   int x = int.parse(stdin.readLineSync());
   for(int i = 0; i< x; i++){
    int n = int.parse(stdin.readLineSync());

    if(n<0 && n%2 != 0){
      print("ODD NEGATIVE");
    }else if(n<0 && n%2 == 0){
       print("EVEN NEGATIVE");
    }else if(n==0){
       print("NULL");
    }else if(n>0 && n%2 == 0){
       print("EVEN POSITIVE");
    }else{
      print("ODD POSITIVE");
    }
   }
}
